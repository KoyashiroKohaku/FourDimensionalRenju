root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4

[*.csproj]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2

[*.sln]
charset = utf-8
end_of_line = lf
indent_style = tab
indent_size = 4
